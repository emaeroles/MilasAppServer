{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    "DevelopmentConnection": "Server=127.0.0.1;Port=5432;Database=milas_app_db;User Id=postgres;Password=yourPassword;"
  },

  "JwtSettings": {
    "SecretKey": "herePutYourKeyIUseAVeryBigKey",
    "Issuer": "https://url-de-la-api.com",
    "Audience": "https://url-de-la-app.com",
    "TokenExpiryInMonths": 1
  },

  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5260"
      }
    }
  },

  "AllowedHosts": "localhost;https://url-de-la-app.com",

  "FilePaths": {
    "LogsFilePath": "yourLogFolderUrl"
  },

  "CorsSettings": {
    "AllowedOrigin": "https://url-de-la-app.com"
  }
}